leta "modules/math.swh"

kazi kuu() {
    andika("Testing modulo by zero...")
    
    jaribu {
        namba result = math.salio(50, 0)
        andika("Result:", result)
    } shika (error) {
        andika("Caught error:", error)
    }
    
    andika("")
